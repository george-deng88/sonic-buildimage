   "COMPONENT1":
    {
        "comp_attr":{ "name": "BIOS", "type": "bios", "description": "Basic Input/Output System"},
        "attr_list":
        [
            { "attr_name":"version", "get_cmd": "echo `dmidecode -s bios-version`-`dmidecode -s bios-release-date`" }
        ]
    },
    "COMPONENT2":
    {
        "comp_attr":{ "name": "BMC", "type": "bmc", "description":"Baseboard Management Controller "},
        "attr_list":
        [
            { "attr_name":"version", "get_cmd": "r=(`ipmitool raw 0x6 0x1 | cut -d ' ' -f 4,5,16,15,14`) && echo ${r[0]}.${r[1]}.${r[4]}.${r[3]}${r[2]}" }
        ]
    },
    "COMPONENT3":
    {
        "comp_attr":{ "name": "BaseBoard_CPLD", "type": "cpld", "description": "Base Board CPLD"},
        "attr_list":
        [
            { "attr_name":"version", "get_cmd": "cat /sys/bus/platform/devices/baseboard/version | sed 's/0x//'" }
        ]
    },
    "COMPONENT4":
    {
        "comp_attr":{ "name": "COMeBoard_CPLD", "type": "cpld", "description": "COMe Board CPLD"},
        "attr_list":
        [
            { "attr_name":"version", "get_cmd": "cat /sys/bus/platform/devices/baseboard/come_cpld_version | sed 's/0x//'" }
        ]
    },
    "COMPONENT5":
    {
        "comp_attr":{ "name": "FPGA", "type": "fpga", "description": "Baseboard FPGA"},
        "attr_list":
        [
            { "attr_name":"version", "get_cmd": "cat /sys/bus/platform/devices/fpga_sysfs/version | sed 's/0x//'" }
        ]
    },
    "COMPONENT6":
    {
        "comp_attr":{ "name": "PCIe", "type": "pcie", "description":"ASIC PCIe Firmware"},
        "attr_list":
        [
            { "attr_name":"version", "get_cmd": "bcmcmd 'pciephy fw version' | grep 'PCIe FW version' | cut -d ' ' -f 4" }
        ]
    },
    "COMPONENT7":
    {
        "comp_attr":{ "name": "SSD", "type": "ssd", "description":"SSD firmware version"},
        "attr_list":
        [
            { "attr_name":"version", "get_cmd": "ssdutil -v | grep 'Firmware' | awk '{ print $3 }'" }
        ]
    },

